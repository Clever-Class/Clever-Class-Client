{
  "compilerOptions": {
    "baseUrl": "./",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "paths": {
      "~api": ["src/api/index"],
      "~api/*": ["src/api/*"],
      "~assets/*": ["src/assets/*"],
      "~components/*": ["src/components/*"],
      "~constants": ["src/constants/index"],
      "~constants/*": ["src/constants/*"],
      "~enumerations": ["src/types/enumerations"],
      "~hooks": ["src/hooks/index"],
      "~pages/*": ["src/pages/*"],
      "~providers": ["src/providers/index"],
      "~providers/*": ["src/providers/*"],
      "~redux": ["src/redux/index"],
      "~redux/*": ["src/redux/*"],
      "~routes": ["src/routes/index"],
      "~routes/*": ["src/routes/*"],
      "~state": ["src/state/index"],
      "~state/*": ["src/state/*"],
      "~store": ["src/store/index"],
      "~store/*": ["src/store/*"],
      "~types": ["src/types/index"],
      "~types/*": ["src/types/*"],
      "~utils": ["src/utils/index"],
      "~utils/*": ["src/utils/*"],
      "~styles/*": ["src/styles/*"],
      "~/*": ["src/*"]
    }
  },
  "include": ["src/**/*"]
}
